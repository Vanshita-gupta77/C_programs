#include <stdio.h>

int main() 
{
    int x,y;
    printf("Enter co ordinates of point\n");
    scanf("%d %d",&x,&y);
    if(x==0 && y!=0)
    {
        printf("Point lies on the y axis");
    }
    else if(y==0 && x!=0)
    {
        printf("Point lies on the x axis");
    }
    else if(x==0 && y==0)
    {
       printf("Point lies on the origin");
    }
    else
    {
        printf("Point lies inside the region");
    }
}
